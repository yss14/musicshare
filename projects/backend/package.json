{
	"name": "@musicshare/backend",
	"version": "1.0.0",
	"description": "",
	"main": "build/index",
	"private": true,
	"scripts": {
		"start": "npm run build && cross-env NODE_ENV=development node build/index.js",
		"start:watch": "tsc-watch --project tsconfig.json --preserveWatchOutput --onSuccess \"cross-env NODE_ENV=development node build/index.js\"",
		"build": "tsc --project tsconfig.json",
		"dev": "concurrently \"npm run azurite\" \"npm run start:watch\"",
		"azurite": "azurite-blob -l azurite",
		"test": "cross-env NODE_ENV=test jest --config=jest.json --detectOpenHandles --colors --maxWorkers=1",
		"test:watch": "jest --watch --config=jest.json --detectOpenHandles --colors",
		"test:cov": "cross-env NODE_ENV=test jest --config=jest.json --collectCoverage --detectOpenHandles --colors --forceExit",
		"test:ci": "npm run test:cov && codecov",
		"lint": "tslint --project tsconfig.json",
		"lint:fix": "tslint --fix --project tsconfig.json",
		"verify": "tsc --noEmit"
	},
	"author": "Yannick Stachelscheid",
	"license": "UNLICENSED",
	"devDependencies": {
		"@types/argon2": "^0.15.0",
		"@types/aws-lambda": "^8.10.23",
		"@types/better-queue": "^3.8.0",
		"@types/body-parser": "^1.17.0",
		"@types/cassandra-driver": "^4.0.3",
		"@types/cors": "^2.8.4",
		"@types/express": "^4.16.1",
		"@types/faker": "^4.1.5",
		"@types/jsonwebtoken": "^8.3.2",
		"@types/mime-types": "^2.1.0",
		"@types/morgan": "^1.7.35",
		"@types/snakecase-keys": "^2.1.0",
		"@types/supertest": "^2.0.7",
		"@types/url-regex": "^5.0.0",
		"@types/ws": "^6.0.1",
		"azurite": "^2.7.0",
		"cross-env": "^5.2.0",
		"dotenv": "^7.0.0",
		"find-free-port": "^2.0.0"
	},
	"dependencies": {
		"@types/graphql": "^14.2.0",
		"apollo-server": "^2.4.8",
		"apollo-server-core": "^2.4.8",
		"apollo-server-express": "^2.4.8",
		"argon2": "^0.21.0",
		"azure-storage": "^2.10.2",
		"better-queue": "^3.8.10",
		"body-parser": "^1.18.3",
		"cassandra-driver": "^4.0.0",
		"cassandra-schema-builder": "^1.0.0",
		"class-transformer": "^0.2.0",
		"cors": "^2.8.5",
		"dotenv": "^7.0.0",
		"express": "^4.16.4",
		"faker": "^4.1.0",
		"graphql": "^14.2.1",
		"id3-parser": "^2.0.0",
		"jsonwebtoken": "^8.5.1",
		"mime-types": "^2.1.22",
		"morgan": "^1.9.1",
		"mp3-duration": "^1.1.0",
		"reflect-metadata": "^0.1.13",
		"similarity": "^1.1.1",
		"snakecase-keys": "^2.1.0",
		"source-map-support": "^0.5.11",
		"supertest": "^4.0.2",
		"ts-node": "^8.0.3",
		"type-graphql": "^0.17.0",
