{
	"name": "musicshare",
	"version": "1.0.0",
	"private": true,
	"description": "Simple yet powerful self-hosted open-source music library, powered by NodeJS, GraphQL, and React.",
	"scripts": {
		"start:backend": "lerna run start --stream --scope @musicshare/backend",
		"start:frontend": "lerna run start --stream --scope @musicshare/frontend",
		"dev": "npm run build:shared-types && lerna run dev --stream --parallel",
		"dev:backend": "lerna run dev --stream --scope @musicshare/backend",
		"bootstrap": "lerna bootstrap",
		"build": "lerna run build --stream",
		"build:backend": "lerna run build --stream --scope @musicshare/backend",
		"build:backend:watch": "lerna run build:watch --stream --scope @musicshare/backend",
		"build:frontend": "lerna run build --stream --scope @musicshare/frontend",
		"build:shared-types": "lerna run build --stream --scope @musicshare/shared-types",
		"build:watch:shared-types": "lerna run build:watch --stream --scope @musicshare/shared-types",
		"build:graphql-client": "lerna run build --stream --scope @musicshare/react-graphql-client",
		"build:watch:graphql-client": "lerna run build:watch --stream --scope @musicshare/react-graphql-client",
		"verify": "lerna run verify --stream",
		"verify:backend": "lerna run verify --stream --scope @musicshare/backend",
		"verify:frontend": "lerna run verify --stream --scope @musicshare/frontend",
		"lint": "eslint ./projects/**/src/ --ext .ts,.tsx,.js,.jsx",
		"lint:fix": "eslint ./projects/**/src/ --ext .ts,.tsx,.js,.jsx --fix",
		"test": "lerna run test --stream",
		"test:backend": "lerna run test --stream --scope @musicshare/backend",
		"test:ci": "lerna run test:ci --stream --scope @musicshare/backend",
		"test:cov": "lerna run test:cov --stream",
		"postinstall": "npm run bootstrap",
		"prettify": "prettier --config .prettierrc.js --ignore-path .prettierignore --write ./projects/**/*.{ts,tsx,js}",
		"generatetypes": "sgts -e http://localhost:4000/graphql -o ./projects/shared-types/src/generatedTypes.ts --customScalars '{\"DateTime\": \"string\", \"OrderUpdate\": \"[string, number]\"}'"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
